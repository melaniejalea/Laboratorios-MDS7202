    "import xgboost as xgb\n",
    "\n",
    "model = xgb.XGBRegressor()\n",
    "\n",
    "pipeline_dos = Pipeline([\n",
    "    ('function_transform', split_func),\n",
    "    ('transformation', ct),\n",
